      "productShowcase": {
        "arrangement": "asymmetric_grid",
        "imageSize": "varied_but_balanced",
        "textPlacement": "below_or_beside"
      },
      "menuLayout": {
        "categories": "subtle_dividers",
        "items": "clean_rows_with_dots",
        "prices": "right_aligned_bold"
      }
    },
  
    "brandPersonality": {
      "keywords": ["优雅", "宁静", "精致", "传统", "季节性", "手工"],
