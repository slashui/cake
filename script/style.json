      "neutral": {
        "darkGray": "#4A4A4A",
        "lightGray": "#8A8A8A",
        "white": "#FFFFFF",
        "description": "深灰色文字，浅灰色辅助信息"
      }
    },
  
    "typography": {
      "primary": {
        "fontFamily": "Noto Sans CJK, 思源黑体",
        "style": "优雅的中日文字体"
      },
      "hierarchy": {
        "mainTitle": {
          "fontSize": "28-36px",
          "fontWeight": "bold",
          "color": "#4A4A4A",
          "orientation": "vertical",
          "description": "主标题使用竖排布局"
        },
        "subtitle": {
          "fontSize": "14-16px", 
          "fontWeight": "normal",
          "color": "#6A6A6A"
