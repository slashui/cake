        "body": {
          "fontSize": "12-14px",
          "fontWeight": "light", 
          "color": "#8A8A8A"
        },
        "price": {
          "fontSize": "18-24px",
          "fontWeight": "bold",
          "color": "#4A4A4A"
        }
      }
    },
  
    "layout": {
      "structure": "asymmetric_grid",
      "principles": {
        "whitespace": "大量留白，营造宁静感",
        "balance": "不对称平衡，突出重点元素",
        "alignment": "左对齐为主，竖排文字右对齐"
      },
      "grid": {
        "type": "flexible_masonry",
        "spacing": "40-60px",
        "margins": "60-80px"
      },
      "composition": {
        "hero": {
          "position": "center_right",
          "elements": ["主图片", "竖排标题", "装饰元素"]
        },
        "productGrid": {
          "layout": "2x3_or_3x2",
          "itemSpacing": "30px",
          "aspectRatio": "square_or_portrait"
        }
      }
    },
  
    "visualElements": {
      "photography": {
        "style": "clean_minimalist",
        "lighting": "soft_natural",
        "background": "neutral_solid",
        "composition": "centered_with_props",
        "props": ["樱花花瓣", "天然材质器皿", "竹制餐具"]
      },
      "decorativeElements": {
        "sakuraPetals": {
          "usage": "scattered_accent",
          "color": "#F4C2C2",
          "size": "small_medium",
          "opacity": "70-85%"
        },
        "geometricShapes": {
          "circles": "subtle_background_elements",
          "lines": "minimal_dividers"
        }
      },
      "textTreatment": {
        "orientation": "vertical_for_titles",
        "alignment": "right_aligned_vertical",
        "spacing": "generous_line_height"
      }
    },
  
    "materialDesign": {
      "textures": {
        "ceramic": "smooth_matte_finish",
        "bamboo": "natural_grain_texture", 
        "fabric": "soft_linen_texture"
      },
      "surfaces": {
        "plates": "dark_slate_or_light_ceramic",
        "background": "soft_fabric_texture",
        "cards": "subtle_paper_texture"
      }
    },
  
    "interactionDesign": {
      "hover": {
        "productCards": "subtle_shadow_lift",
        "images": "gentle_scale_105%",
        "buttons": "color_fade_transition"
      },
      "transitions": {
        "duration": "300-500ms",
        "easing": "ease_in_out",
        "style": "smooth_organic"
      }
    },
  
    "contentStrategy": {
      "pricing": {
        "display": "prominent_clean_typography",
        "currency": "dollar_or_yen_symbols",
        "alignment": "right_aligned"
